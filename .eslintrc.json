{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:@next/next/recommended"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": ["error", "never"],
    "indent": ["error", 2],
    "react/jsx-curly-newline": [
      "error",
      { "multiline": "consistent", "singleline": "consistent" }
    ]
  }
}
